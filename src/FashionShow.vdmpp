class FashionShow
instance variables 
	public name:Utils`string;
  public place:Utils`string;
  public startDate:Utils`date;
  public endDate:Utils`date

operations
-- constructor
	public FashionShow : Utils`string * Utils`string * Utils`date * Utils`date ==> FashionShow
