class RunwayShow is subclass of Event
instance variables
  theme:Utils`string;

operations
-- constructor
	public RunwayShow : Room * Utils`string * Utils`date * Utils`date * Utils`string ==> RunwayShow
		RunwayShow(r, n, sD, eD, t) == (
			organizers := [];
 			place := r;
 			name := n;
 			startDate := sD;
 			endDate := eD;
			theme := t;
			return self;		
		);
		
-- set name
 	public setTheme : Utils`string ==> ()
 		setTheme(t) ==
 			theme := t;
		
end RunwayShow
