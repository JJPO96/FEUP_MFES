class Room
instance variables
  name:Utils`string;
  localization:Utils`string;

operations
-- constructor
	public Room : Utils`string * Utils`string ==> Room
		Room(n, local) == (
			name := n;
			localization	:= local;
			return self;
		);
		
-- set name
 	public setName : Utils`string ==> ()
 		setName(n) ==
 			name := n;
 			
-- set localization
 	public setLocalization : Utils`string ==> ()
 		setLocalization(l) ==
 			localization := l;
 			
end Room
