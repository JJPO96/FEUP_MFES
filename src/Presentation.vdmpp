class Presentation is subclass of Event
instance variables
  public subject:Utils`string;
  
operations
-- constructor
	public Presentation : Room * Utils`string * Utils`date * Utils`date * Utils`string ==> Presentation
	Presentation(r, n, sD, eD, s) == (
		organizers := [];
		place := r;
		name := n;
		startDate := sD;
		endDate := eD;
		subject := s;
		return self;		
	);

-- set subject	
	public setSubject : Utils`string ==> ()
		setSubject(s) ==
			subject := s;

end Presentation
