			Utils`assertTrue(fasTest1.endDate = mk_Utils`date(2017,12,20,01,00));
			
			--test setEvents()
			fasTest1.setEvents([preTest1,runTest1]);			
			Utils`assertTrue(fasTest1.events = [preTest1,runTest1]);		
		);
		
